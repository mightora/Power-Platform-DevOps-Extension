{
    "manifestVersion": 1,
    "id": "mightora-power-platform-devOps-extension-dev",
    "name": "DEV - Mightora.io Power Platform DevOps Extension",
    "version": "1.0.202",
    "publisher": "mightoraio",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Power Platform Documentation Extension, allows users of this extension to quickly and easily document out their solutions in to easy to read documentation.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/mightora_io_icon_260x260.png"
    },
    "content": {
        "details": {
            "path": "details.md"
        },
        "pricing": {
            "path": "pricing.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://mightora.io"
        },
        "getstarted": {
            "uri": "https://mightora.io"
        },
        "learn": {
            "uri": "https://mightora.io"
        },
        "support": {
            "uri": "https://mightora.io"
        },
        "repository": {
            "uri": "https://mightora.io"
        },
        "issues": {
            "uri": "https://mightora.io"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/mightora"
    },
    "files": [
        {
            "path": "tasks/test",
            "packagePath": "test"
        },
        {
            "path": "tasks/documentSolutionTables",
            "packagePath": "documentSolutionTables"
        },
        {
            "path": "tasks/commitToRepo",
            "packagePath": "commitToRepo"
        },
        {
            "path": "tasks/documentTableRelationships",
            "packagePath": "documentTableRelationships"
        },
        {
            "path": "tasks/solutionXMLNodeUpdater",
            "packagePath": "solutionXMLNodeUpdater"
        },
        {
            "path": "tasks/solutionXMLNodeAttributeUpdater",
            "packagePath": "solutionXMLNodeAttributeUpdater"
        },
        {
            "path": "tasks/documentSolutionManifest",
            "packagePath": "documentSolutionManifest"
        },
        {
            "path": "tasks/documentOptionSets",
            "packagePath": "documentOptionSets"
        },
        {
            "path": "tasks/documentWorkflows",
            "packagePath": "documentWorkflows"
        }
    ],
    "contributions": [
        {
            "id": "mightora-test-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "test"
            }
        },
        {
            "id": "mightora-documentSolutionTables-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "documentSolutionTables"
            }
        },
        {
            "id": "mightora-commitToRepo-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "commitToRepo"
            }
        },
        {
            "id": "mightora-documentTableRelationships-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "documentTableRelationships"
            }
        },
        {
            "id": "mightora-solutionXMLNodeUpdater-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "solutionXMLNodeUpdater"
            }
        },
        {
            "id": "mightora-solutionXMLNodeAttributeUpdater-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "solutionXMLNodeAttributeUpdater"
            }
        },
        {
            "id": "mightora-documentSolutionManifest-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "documentSolutionManifest"
            }
        },
        {
            "id": "mightora-documentOptionSets-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "documentOptionSets"
            }
        },
        {
            "id": "mightora-documentWorkflows-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "documentWorkflows"
            }
        }
    ]
}