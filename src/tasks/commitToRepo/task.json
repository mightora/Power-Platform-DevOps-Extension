{
    "$schema": "https://raw.githubusercontent.com/Microsoft/azure-pipelines-task-lib/master/tasks.schema.json",
    "id": "579190ac-ccd4-4451-a199-a6150001cf1a",
    "name": "commitToRepo",
    "friendlyName": "(DEPRECATED) Commit To Git Repo",
    "instanceNameFormat": "Commit To Git Repository",
    "description": "Moved to https://marketplace.visualstudio.com/items?itemName=mightoraio.mightora-commit-to-repo-extension",
    "helpMarkDown": "See [updated extension](https://marketplace.visualstudio.com/items?itemName=mightoraio.mightora-commit-to-repo-extension)",
    "category": "Build",
    "author": "mightoraio",
    "version": {
        "Major": 2,
        "Minor": 2,
        "Patch": 14
    },
    "groups": [
        {
            "name": "inputGroup",
            "displayName": "Source",
            "isExpanded": true
        },
        {
            "name": "outputGroup",
            "displayName": "Output",
            "isExpanded": true
        }
    ],
    "inputs": [
        {
            "name": "commitMsg",
            "type": "string",
            "label": "Commit message",
            "defaultValue": "",
            "required": true,
            "helpMarkDown": "Commit message",
            "groupName": "inputGroup"
        },
        {
            "name": "branchName",
            "type": "string",
            "label": "Branch name",
            "defaultValue": "main",
            "required": true,
            "helpMarkDown": "Name of the branch to commit to",
            "groupName": "inputGroup"
        },
        {
            "name": "tags",
            "type": "string",
            "label": "Tags",
            "defaultValue": "",
            "required": false,
            "helpMarkDown": "Comma-separated list of tags to add to the commit",
            "groupName": "inputGroup"
        }
    ],
    "execution": {
        "PowerShell3": {
            "target": "powershell.ps1",
            "platforms": [
                "windows"
            ],
            "workingDirectory": "$(currentDirectory)"
        }
    }
}